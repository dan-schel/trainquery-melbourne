{
  "type": "module",
  "dependencies": {
    "@dan-schel/js-utils": "4.9.0",
    "@eslint/js": "9.39.3",
    "@types/adm-zip": "0.5.7",
    "@types/node": "25.3.0",
    "@vitest/coverage-v8": "4.0.18",
    "adm-zip": "0.5.16",
    "chalk": "5.6.2",
    "corequery": "0.5.6",
    "csv-parser": "3.2.0",
    "dotenv": "17.3.1",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "knip": "5.85.0",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18",
    "zod": "4.3.6"
  },
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "dev-corequery-demo-app": "tsx watch --clear-screen=false --exclude ../node_modules src/index.ts",
    "start": "tsx src/index.ts",
    "lint": "tsc -p tsconfig.json --noEmit && eslint . --max-warnings 0 && knip",
    "test": "vitest --run",
    "test-coverage": "vitest --run --coverage",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "compare-config-to-gtfs": "tsx scripts/compare-config-to-gtfs/main.ts",
    "download-gtfs": "tsx scripts/download-gtfs/main.ts",
    "import-stop-from-gtfs": "tsx scripts/import-stop-from-gtfs/main.ts"
  },
  "corequeryDemoApp": {
    "scripts": {
      "dev": "dev-corequery-demo-app",
      "start": "start"
    },
    "environmentVariables": [
      {
        "name": "DATABASE_URL"
      },
      {
        "name": "VTAR_KEY"
      }
    ]
  }
}
